extends ../../layout/index.pug
include ../../mixi/mixi.pug
block main
      .pc-content(style="padding : 0;")
        // [ breadcrumb ] start
        .page-header
          .page-block
            .row.align-items-center
              .col-md-12
                .page-header-title
                  h2.mb-0 Kiểm Duyệt Tài Khoảng
        .row
          each item in dataaccount
             .col-sm-6.col-xl-4
                .card.statistics-card-1
                  .card-header.d-flex.align-items-center.justify-content-between.py-3
                    h5 #{item.fullName}
                    .dropdown
                      a.avtar.avtar-xs.btn-link-secondary.dropdown-toggle.arrow-none(href='#' data-bs-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
                        i.material-icons-two-tone.f-18 more_vert
                      .dropdown-menu.dropdown-menu-end
                        a.dropdown-item(href=`/admin/accountreview/check/${item.id}`) Xem Chi tiết
                        a.dropdown-item(href='#') Xóa
                  .card-body
                    img.img-fluid.img-bg.h-100(src='/admin/layoutadmin/images/img-status-1.svg' alt='img')
                    .d-flex.align-items-center
                      h3.f-w-300.d-flex.align-items-center.m-b-0
                        small.text-muted.m-r-10 Ngày Đăng Ký
                      span.badge.bg-light-success.ms-2 #{item.time}
                    p.text-muted.mb-2.text-sm.mt-3 Số điện Thoại #{item.phone}
                    .progress(style='height: 7px')
                      .progress-bar.bg-brand-color-1(role='progressbar' style='width: 100%'  aria-valuemin='0' aria-valuemax='100')
